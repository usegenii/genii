{
	"name": "@geniigotchi/models",
	"version": "1.0.0",
	"description": "Model factory bridging config and adapters for geniigotchi",
	"type": "module",
	"main": "./dist/factory.js",
	"types": "./dist/factory.d.ts",
	"exports": {
		"./*": {
			"import": "./dist/*.js",
			"types": "./dist/*.d.ts"
		}
	},
	"files": [
		"dist"
	],
	"scripts": {
		"build": "tsup",
		"dev": "tsup --watch",
		"check": "biome check .",
		"check:fix": "biome check --write .",
		"typecheck": "tsc --noEmit",
		"test": "vitest run",
		"test:watch": "vitest"
	},
	"keywords": [],
	"author": "",
	"license": "ISC",
	"packageManager": "pnpm@10.24.0",
	"dependencies": {
		"@geniigotchi/config": "workspace:*",
		"@geniigotchi/orchestrator": "workspace:*"
	},
	"devDependencies": {
		"@types/node": "^22.15.21",
		"tsup": "8.5.1",
		"typescript": "5.9.3",
		"vitest": "^3.2.2"
	}
}
