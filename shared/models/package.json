{
	"name": "@genii/models",
	"version": "1.0.0",
	"description": "Model factory bridging config and adapters for genii",
	"type": "module",
	"main": "./dist/factory.js",
	"types": "./dist/factory.d.ts",
	"exports": {
		"./*": {
			"import": "./dist/*.js",
			"types": "./dist/*.d.ts"
		}
	},
	"files": [
		"dist"
	],
	"scripts": {
		"build": "tsup",
		"dev": "tsup --watch",
		"check": "biome check .",
		"check:fix": "biome check --write .",
		"typecheck": "tsc --noEmit",
		"test": "vitest run",
		"test:watch": "vitest"
	},
	"keywords": [],
	"author": "",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/usegenii/genii"
	},
	"packageManager": "pnpm@10.30.2",
	"dependencies": {
		"@genii/config": "workspace:*",
		"@genii/orchestrator": "workspace:*"
	},
	"devDependencies": {
		"@types/node": "24.10.13",
		"tsup": "8.5.1",
		"typescript": "5.9.3",
		"vitest": "3.2.4"
	}
}
