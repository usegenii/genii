{
	"name": "@genii/cli",
	"version": "1.0.0",
	"description": "CLI for managing genii agents",
	"type": "module",
	"main": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"bin": {
		"genii": "./dist/genii.js"
	},
	"exports": {
		".": {
			"import": "./dist/index.js",
			"types": "./dist/index.d.ts"
		},
		"./genii": "./dist/genii.js"
	},
	"files": [
		"dist"
	],
	"scripts": {
		"build": "tsup",
		"dev": "tsx watch bin/genii.ts",
		"check": "biome check .",
		"check:fix": "biome check --write .",
		"typecheck": "tsc --noEmit",
		"test": "vitest run",
		"test:watch": "vitest"
	},
	"keywords": [
		"cli",
		"ai",
		"agents"
	],
	"author": "",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/usegenii/genii"
	},
	"packageManager": "pnpm@10.30.2",
	"dependencies": {
		"@genii/config": "workspace:*",
		"chalk": "5.6.2",
		"cli-table3": "0.6.5",
		"commander": "12.1.0",
		"ink": "5.2.1",
		"ink-select-input": "6.2.0",
		"ink-text-input": "6.0.0",
		"ora": "8.2.0",
		"react": "18.3.1"
	},
	"devDependencies": {
		"@types/node": "24.10.13",
		"@types/react": "^18.0.0",
		"tsup": "8.5.1",
		"typescript": "5.9.3",
		"vitest": "3.2.4"
	}
}
